{
	"overrides": [
		{
			"files": "*.sol",
			"options": {
				"printWidth": 120,
				"tabWidth": 4,
				"useTabs": true,
				"singleQuote": false,
				"bracketSpacing": false,
				"explicitTypes": "preserve"
			}
		},
		{
			"files": "*.json",
			"options": {
				"tabWidth": 4,
				"useTabs": true,
				"singleQuote": false
			}
		}
	]
}
